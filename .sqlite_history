select * from tb_base_sandwich;
.headers on
select * from tb_base_sandwich;
  select name,
         can_customize_cheese,
         can_customize_extras,
         can_customize_toppings,
         can_customize_condiments,
         can_customize_heating_option
    from tb_base_sandwich
order by random()
   limit 1;
  select name,
         can_customize_cheese,
         can_customize_extras,
         can_customize_toppings,
         can_customize_condiments,
         can_customize_heating_option
    from tb_base_sandwich
order by random()
   limit 1;
  select name,
         can_customize_cheese,
         can_customize_extras,
         can_customize_toppings,
         can_customize_condiments,
         can_customize_heating_option
    from tb_base_sandwich
order by random()
   limit 1;
.exit
